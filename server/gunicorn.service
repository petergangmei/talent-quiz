[Unit]
Description=Gunicorn daemon for Django app
After=network.target

[Service]
Environment="DJANGO_SETTINGS_MODULE=core.settings.prod"
User=ubuntu
WorkingDirectory=/home/ubuntu/tq
ExecStart=/home/ubuntu/tq/venv/bin/gunicorn -k gevent --workers 1 --timeout 30 --bind unix:/home/ubuntu/tq/app.sock core.wsgi:application

[Install]
WantedBy=multi-user.target